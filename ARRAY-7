#include <stdio.h>
int main()
{
   int size;
   scanf("%d",&size);
   int UC=0,LC=0,SPL=0,D=0;
   char arr[size];
   for(int i=0;i<size;i++)
   {
       scanf(" %c",&arr[i]);
   }
   for(int i=0;i<size;i++)
   {
       if(arr[i]>='A'&&arr[i]<='Z')
       {
           UC++;
       }
       else if(arr[i]>='a'&&arr[i]<='z')
       {
           LC++;
       }
       else if((arr[i]>='0'&&arr[i]<='9'))
       {
           D++;
           
       }
       else
       {
           SPL++;
       }
       
   }
   printf("UC=%d\nLC=%d\nD=%d\nSPL=%d",UC,LC,D,SPL);
    return 0;
}
