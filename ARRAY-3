//To find sum of elements in an array 
#include <stdio.h>
int main()
{
    int n;
    int sum;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    int elements[n];
    printf("Enter the elements:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&elements[i]);
    }
    printf("The elements in the array are:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d\n",elements[i]);
    }
    for(int i=0;i<n;i++)
    {
        sum+=elements[i];
    }
    printf("The sum of elements are %d",sum);
    return 0;
}
