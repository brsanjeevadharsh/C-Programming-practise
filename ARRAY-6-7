#include <stdio.h>
int main()
{
int n;
printf("Enter the no of elements:");
scanf("%d",&n);
int a[n];
int o[n];
int e[n];
for(int i=0;i<n;i++)
{
    scanf("%d",&a[i]);
}

for(int i=0;i<n;i++)
{
    if(a[i]%2==0)
    {
      printf("Even Numbers: %d\n",a[i]);
    }
}
for(int i=0;i<n;i++)
{
    if(a[i]%2!=0)
    {
      printf("Odd Numbers: %d\n",a[i]);
    }
}  
return 0;
}
