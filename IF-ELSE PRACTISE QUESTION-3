#include <stdio.h>
int main()
{
    int num1,num2;
    printf("enter the number1:");
    scanf("%d",&num1);
    printf("enter the number2:");
    scanf("%d",&num2);
    if(num1==num2)
    {
        printf("Number 1 is equal to Number 2");
    }
    else
    {
        printf("Number 1 is not equal to Number 2");
    }
}
