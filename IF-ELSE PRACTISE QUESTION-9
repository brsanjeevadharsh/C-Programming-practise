#include <stdio.h>
int main()
{
    printf("Enter the three numbers:\n");
    int num1,num2,num3;
    scanf("%d\n",&num1);
    scanf("%d\n",&num2);
    scanf("%d",&num3);
    if(num1>num2 && num1>num3)
    {
        printf("Number %d is bigger",num1);
    }
    else if(num2>num1 && num2>num3)
    {
        printf("Number %d is bigger",num2);
    }
    else
    {
        printf("Number %d is bigger",num3);
    }
    return 0;
}
