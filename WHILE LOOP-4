//professor logic
#include <stdio.h>
int main()
{
    int a=2;
    while(a<=10)
    {
        if(a%2==0)
        {
        printf("%d\n",a);
        }
        a++;
    }
    return 0;
}

//my logic
#include <stdio.h>
int main()
{
    int a=2;
    while(a<=10)
    {
        printf("%d\n",a);
        a+=2;
    }
    return 0;
}
