//product of even natural numbers
#include <stdio.h>
int main()
{
    int s=1;
    int e=5;
    int product = 1;
    while(s<=e)
    {
        if(s%2==0)
        {
            product = product * s;
        }
        s++;
    }
    printf("Product of even natural numbers is %d",product);
    return 0;
}
