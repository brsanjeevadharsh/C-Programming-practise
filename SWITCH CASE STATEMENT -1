#include <stdio.h>
int main()
{
    int month;
   printf("Enter the Month Number:");
   scanf("%d",&month);
    switch(month)
    {
        case 1:
        printf("January");
        case 2:
        printf("February");
        break;
        case 3:
        printf("March");
        case 4:
        printf("April");
        break;
        case 5:
        printf("May");
        case 6:
        printf("June");
        case 7:
        printf("July");
        break;
        case 8:
        printf("August");
        case 9:
        printf("September");
        break;
        case 10:
        printf("October");
        case 11:
        printf("November");
        break;
        case 12:
        printf("December");
        break;
        default:
            printf("Invalid credential");
    }
}
