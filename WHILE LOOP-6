#include <stdio.h>
int main()
{
    int a=10;
    while(a<=10 && a>0)
    {
        printf("%d\n",a);
        a--;
    }
    return 0;
}

//updated new code by my ashokkumar professor 
#include <stdio.h>
int main()
{
    int a=10;
    while(a>0)
    {
        printf("%d\n",a);
        a--;
    }
    return 0;
}
